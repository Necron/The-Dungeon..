void SetWindow(int Width, int Height){ 
   _COORD coord; 
   coord.X = Width; 
   coord.Y = Height; 
   SMALL_RECT Rect; 
   Rect.Top = 0; 
   Rect.Left = 0; 
   Rect.Bottom = Height - 1; 
   Rect.Right = Width - 1; 
   HANDLE Handle = GetStdHandle(STD_OUTPUT_HANDLE);      // Get Handle 
   SetConsoleScreenBufferSize(Handle, coord);            // Set Buffer Size 
   SetConsoleWindowInfo(Handle, TRUE, &Rect);            // Set Window Size 
} 
void FWD();//forward motion art
void LFT();// left turn art
void RGT();// right turn art
void FGT(void); //fight scene art
void deadend();//dead end art
