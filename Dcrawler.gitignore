void begining();//introduction
void intro();
void CN();//compas direction
void CS();
void CW();
void CE();
void BWD();//reverse movement sequence
void HALL();//after turn image

int D = 150;//frame speed
int S = 5;  //frame repetitions
main(){
   int EXP = 0;
   SetWindow(75,40);
   system("color 6");
   int H=0,ATK=10,DEF=5;
   int HP=1,AP=1,SP=1,HLTH=10,STR=1,DEX=1;
   begining();
   Sleep(1500);
   char dir;
   char hero[80];
   printf("   What is your name hero? \n");
   gets(hero);
   printf("\n initializing random stat generator....");
   Sleep(1000);
   int x=1;
   int y=5;
   #define randnum(min, max) \
   ((rand()%(int)(((max) + 1)-(min)))+ (min)) //this snipet of code i borrowed from coderperson on stack overflow
   srand (time(NULL));
   rand()%100;
   STR = STR + randnum(x,y);
   Sleep(200);
   DEX = DEX + randnum(x,y);
   Sleep(200);
   HLTH=10+randnum(x,y);   
   printf("\n DEXTERITY = %d",DEX);
   printf("\n STRENGTH = %d",STR);
   printf("\n HEALTH = %d\n", HLTH); 
   Sleep(500);
   intro();
   printf("\n Enter ye direction %s\n",hero);
   printf("\n w=forward a=left d=right s=back hit enter\n");
   printf("\n h=hlth pot r=str pot g=agile pot hit enter\n");
   ATK=ATK+STR;
   DEF=DEF+DEX;
   while(1){
      int s = 0;            
      scanf("%c",&dir);
      switch(dir){
         case 'h':
            HLTH=HLTH+7;
            printf("\n Health has increased by 7!\n");
            break;
         case 'r':
            STR=STR+1;
            printf("\n Strength has increased by 1!\n");
            break;
         case 'g':
            DEF=DEF+3;
            printf("\n Defense has increased by 3!\n");
            break;
         case 'w':
         //forward sequence
            for(s=0;s<S;s++){
               FWD();               
            }                   
         //end forward sequence         
            printf("\n*=====================================================================*");
            printf("\n  %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d      ",hero, HLTH,ATK,DEF,EXP);
            printf("\n*=====================================================================*");
            INV();
            printf("\n*=====================================================================*");
            printf("\n| HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d  |", HP,SP,AP);
            printf("\n*=====================================================================*\n"); 
         //random dungeon mapper 
            int LW=1;
            int HGH=10;
            int rndy;
            #define randnum(min, max) \
            ((rand()%(int)(((max) + 1)-(min)))+ (min)) //this snipet of code i borrowed from coderperson on stack overflow
            srand (time(NULL));
            rand()%100;
            rndy = randnum(LW,HGH);     
            if(rndy>5){
                  bool flag = 1;
                  char dire;
                  int L=1;
                  int H=40;
                  int rdy;
                  #define randnum(min, max) \
                  ((rand()%(int)(((max) + 1)-(min)))+ (min)) //this snipet of code i borrowed from coderperson on stack overflow
                  srand (time(NULL));
                  rand()%100;
                  rdy = randnum(L,H);     
                 if(rdy>30){
                    LFT();
                    printf("\n*=====================================================================*");
                    printf("\n  %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d      ",hero, HLTH,ATK,DEF,EXP);
                    printf("\n*=====================================================================*");
                    INV();
                    printf("\n*=====================================================================*");
                    printf("\n| HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d  |", HP,SP,AP);
                    printf("\n*=====================================================================*\n");
                    printf("\n Ye have encountered a left turn!\n");
                    while(flag){
                       scanf(" %c",&dire);
                       switch(dire){
                          case 'w':
                             printf("\n Ye cannot venture through the wall!");
                             break;
                          case 'a':
                             HALL();
                             printf("\n Ye venture to the left!");
                             break;
                          case 'd':
                             printf("\n Ye cannot venture to the right!");
                             break;
                          case 's':
                             printf("\n Ye travel backwards!");
                             BWD();
                             break;              
                       }
                       if(dire=='s'){
                          flag=0;
                       }
                       if(dire=='a'){
                          flag=0;
                       }
                    }     
                 }
                 if(rdy<30&&rdy>20){
                    RGT();
                    printf("\n*=====================================================================*");
                    printf("\n  %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d      ",hero, HLTH,ATK,DEF,EXP);
                    printf("\n*=====================================================================*");
                    INV();
                    printf("\n*=====================================================================*");
                    printf("\n| HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d  |", HP,SP,AP);
                    printf("\n*=====================================================================*\n");                    
                    printf("\n Ye have encountered a right turn!\n");
                    while(flag){
                       scanf(" %c",&dire);
                       switch(dire){
                          case 'w':
                             printf("\n Ye cannot venture through the wall!");
                             break;
                          case 'a':
                             printf("\n Ye cannot venture to the left!");
                             break;
                          case 'd':
                             HALL();
                             printf("\n Ye venture to the right!");
                             break;
                          case 's':
                             printf("\n Ye travel backwards!");
                             BWD();
                             break;              
                       } 
                       if(dire=='s'){
                          flag=0;
                       }
                       if(dire=='d'){
                          flag=0;
                       }
                    }        
                 }
                 if(rdy<20&&rdy>10){
                    deadend();
                    printf("\n*=====================================================================*");
                    printf("\n  %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d      ",hero, HLTH,ATK,DEF,EXP);
                    printf("\n*=====================================================================*");
                    INV();
                    printf("\n*=====================================================================*");
                    printf("\n| HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d  |", HP,SP,AP);
                    printf("\n*=====================================================================*\n");
                    printf("\n Ye have encountered a dead end!\n");
                    while(flag){
                       scanf(" %c",&dire);
                       switch(dire){
                          case 'w':
                             printf("\n Ye cannot venture through the wall!");
                             break;
                          case 'a':
                             printf("\n Ye cannot venture to the left!");
                             break;
                          case 'd':
                             printf("\n Ye cannot venture to the right!");
                             break;
                          case 's':
                             printf("\n Ye travel backwards!");
                             BWD();
                             break;              
                       }
                       if(dire=='s'){
                          flag=0;
                       }
                    }              
                 }            
            }
         //end randum dungeon mapper
            
         //fight generator              
            int LOW=1;
            int HIGH=10;
            int randy;
            #define randnum(min, max) \
            ((rand()%(int)(((max) + 1)-(min)))+ (min)) //this snipet of code i borrowed from coderperson on stack overflow
            srand (time(NULL));
            rand()%100;
            randy = randnum(LOW,HIGH);     
            if(randy>7){            
               FGT();
               printf("\n*=====================================================================*");
               printf("\n  %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d      ",hero, HLTH,ATK,DEF,EXP);
               printf("\n*=====================================================================*");
               INV();
               printf("\n*=====================================================================*");
               printf("\n  HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d   ", HP,SP,AP);
               printf("\n*=====================================================================*\n");
               printf("\n YE HAVE BEEN AMBUSHED, PREPARE TO FIGHT!\n"); 
               int ENMY = randy+1;
               printf("\n ENEMY HAS %d HEALTH",ENMY);
               while(1){
                  char ACT;            
                  printf("\n select q to strike, h to drink health potion");
                  printf("\n r for strength potion, g for agility potion\n");
                  scanf(" %c",&ACT);               
                  switch(ACT){
                     case 'h':
                        HLTH=HLTH+7;
                        break;              
                     case 's':
                        ATK=ATK+1;
                        break;
                     case 'g':
                        DEF=DEF+3;
                        break;
                     case 'q':
                        ENMY=ENMY-ATK;
                        printf("\n THE ENEMY HAS %d points remaining",ENMY);
                        break;
                     }
                     if(ENMY<1){
                        EXP=EXP+3;
                        printf("\n you have slain the enemy! you receive 3 EXP");
                        break;
                     }
                     if(HLTH<1){
                        death();
                     return 0;
                     }
                     if(EXP==21){
                        printf("\n Ye have gained a level!\n");
                        HLTH=HLTH+3;
                        DEX=DEX+1;
                        STR=STR+1;
                     }                        
                     HLTH=HLTH -(randy);
                     FGT();
                     printf("\n*=====================================================================*");
                     printf("\n| %s           Health = %d   ATTACK = %d  DEFENSE = %d   EXP = %d     |",hero, HLTH,ATK,DEF,EXP);
                     printf("\n*=====================================================================*");
                     INV();
                     printf("\n*=====================================================================*");
                     printf("\n| HEALTH POTIONS = %d   STRENTH POTIONS = %d    AGILITY POTIONS = %d  |", HP,SP,AP);
                     printf("\n*=====================================================================*\n");
                     printf("\n THE ENEMY ATTACKS!"); 
                  }                                                           
               }         
               break;
         //end fight gen              
      }                
   }
}
   

